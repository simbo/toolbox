{
  "name": "toolbox",
  "version": "0.1.0",
  "description": "Some online tools.",
  "author": "Simon Lepel <simbo@simbo.de> (http://simbo.de)",
  "homepage": "https://simbo.codes/toolbox",
  "license": "MIT",
  "private": true,
  "repository": "github:simbo/toolbox",
  "bugs": "https://github.com/simbo/toolbox/issues",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "ci:build": "ng build --prod --no-progress",
    "test": "ng test --browsers=ChromeHeadlessNoSandbox",
    "ci:test": "ng test --no-watch --no-progress --code-coverage --browsers=ChromeHeadlessNoSandbox",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "ng lint && yarn run lint:styles",
    "lint:styles": "stylelint \"./src/**/*.(c|sc|sa)ss\"",
    "e2e": "ng e2e",
    "analyzer": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "check": "yarn run lint && yarn run ci:test && yarn run ci:build",
    "preinstall": "npx use-yarn",
    "postinstall": "node ./.ng-cli-webpack-hack/hack.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run check"
    }
  },
  "engines": {
    "node": ">=8.11.2"
  },
  "dependencies": {
    "@angular/animations": "^6.0.9",
    "@angular/common": "^6.0.9",
    "@angular/compiler": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/forms": "^6.0.9",
    "@angular/http": "^6.0.9",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@angular/router": "^6.0.9",
    "autosize": "^4.0.2",
    "camelcase": "^5.0.0",
    "color": "^3.0.0",
    "core-js": "^2.5.4",
    "crypto-browserify": "^3.12.0",
    "css-reset-and-normalize": "^1.0.1",
    "delta-e": "^0.0.7",
    "is-number": "^7.0.0",
    "lz-string": "^1.4.4",
    "random-int": "^1.0.0",
    "rxjs": "^6.0.0",
    "shortid": "^2.2.11",
    "slugify": "^1.3.0",
    "store": "^2.0.12",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.9",
    "@angular/language-service": "^6.0.9",
    "@types/color": "^3.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.10.21",
    "autoprefixer": "^8.6.5",
    "codelyzer": "~4.4.2",
    "color-name-list": "^3.46.0",
    "coveralls": "^3.0.2",
    "css-mqpacker": "^6.0.2",
    "css-mqpacker-sort-mediaqueries": "^0.2.2",
    "html-minifier": "^3.5.18",
    "husky": "^1.0.0-rc.9",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.2.0",
    "karma-mocha-reporter": "^2.2.5",
    "marked": "^0.4.0",
    "protractor": "~5.3.0",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-scss": "^3.1.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2",
    "use-yarn": "^2.2.0",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
