{
  "name": "toolbox",
  "version": "0.1.0",
  "description": "Some online tools.",
  "author": "Simon Lepel <simbo@simbo.de> (http://simbo.de)",
  "homepage": "https://simbo.codes/toolbox",
  "license": "MIT",
  "private": true,
  "repository": "github:simbo/toolbox",
  "bugs": "https://github.com/simbo/toolbox/issues",
  "engines": {
    "node": "~10.15.0"
  },
  "scripts": {
    "start": "ng serve",
    "build": "ng build --prod --configHook ~webpack.config-hook.js",
    "ci:build": "yarn run build --no-progress",
    "test": "ng test --configHook ~webpack.config-hook.js --browsers=ChromeHeadlessNoSandbox",
    "ci:test": "yarn run test --no-watch --no-progress --code-coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "yarn run lint:scripts && yarn run lint:styles",
    "lint:scripts": "ng lint",
    "lint:styles": "stylelint \"./src/**/*.(c|sc|sa)ss\"",
    "e2e": "ng e2e",
    "analyzer": "yarn run build --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "check": "yarn run lint && yarn run ci:test && yarn run ci:build",
    "preinstall": "npx use-yarn"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run check"
    }
  },
  "dependencies": {
    "@angular/animations": "^7.2.1",
    "@angular/common": "^7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/core": "^7.2.1",
    "@angular/forms": "^7.2.1",
    "@angular/http": "^7.2.1",
    "@angular/platform-browser": "^7.2.1",
    "@angular/platform-browser-dynamic": "^7.2.1",
    "@angular/router": "^7.2.1",
    "array-shuffle": "^1.0.1",
    "array-unique": "^0.3.2",
    "autosize": "^4.0.2",
    "camelcase": "^5.0.0",
    "color": "^3.1.0",
    "core-js": "^2.6.2",
    "crypto-browserify": "^3.12.0",
    "css-reset-and-normalize": "^1.2.0",
    "delta-e": "^0.0.7",
    "is-number": "^7.0.0",
    "lz-string": "^1.4.4",
    "random-int": "^1.0.0",
    "random-item": "^1.0.0",
    "rxjs": "^6.3.3",
    "shortid": "^2.2.14",
    "slugify": "^1.3.4",
    "store": "^2.0.12",
    "unique-random-array": "^1.0.1",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.28"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.2",
    "@angular/cli": "~7.2.2",
    "@angular/compiler-cli": "^7.2.1",
    "@angular/language-service": "^7.2.1",
    "@types/color": "^3.0.0",
    "@types/jasmine": "~3.3.7",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.18",
    "autoprefixer": "^9.4.5",
    "codelyzer": "~4.5.0",
    "color-name-list": "^3.52.1",
    "coveralls": "^3.0.2",
    "css-mqpacker": "^7.0.0",
    "css-mqpacker-sort-mediaqueries": "^0.2.2",
    "html-minifier": "^3.5.21",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-mocha-reporter": "^2.2.5",
    "marked": "^0.6.0",
    "ngx-build-plus": "^7.7.0",
    "protractor": "~5.4.2",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylelint": "^9.10.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-scss": "^3.5.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.1",
    "typescript": "~3.2.4",
    "use-yarn": "^2.2.0",
    "webpack-bundle-analyzer": "^3.0.3"
  }
}
